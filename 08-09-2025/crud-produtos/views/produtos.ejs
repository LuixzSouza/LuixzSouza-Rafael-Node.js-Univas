<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Nome</th>
                <th>Tipo</th>
            </tr>
        </thead>
        <tbody>
            <% produtos.forEach(produtos => { %>
                <tr>
                    <td><%= produtos.id %></td>    
                    <td><%= produtos.nome %></td>    
                    <td><%= produtos.tipo %></td>    
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>